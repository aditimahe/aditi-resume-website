    "clean": "^4.0.2",
    "jquery": "^3.5.1",
    "popper.js": "^1.14.3"
  },
  "devDependencies": {
    "autoprefixer": "10.0.1",
    "browser-sync": "2.26.13",
    "chokidar": "3.4.3",
    "concurrently": "5.3.0",
    "postcss": "8.1.4",
    "prettier": "2.1.2",
    "pug": "3.0.0",
    "sass": "1.28.0",
